	<queryString language="SQL">
		<![CDATA[SELECT TITULO,
	FECHA_PRESTAMO,
	FECHA_ENTREGA,
	NOMBRE,
	APELLIDO_PAT,
	APELLIDO_MAT
FROM PRESTAMO
	JOIN DETALLES_PRESTAMO USING(PRESTAMO_ID) 
	JOIN LIBRO USING(LIBRO_ID) 
	JOIN CLIENTE USING(CLIENTE_ID)
