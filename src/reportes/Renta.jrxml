	<parameter name="ID_RENTA" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT RENTAL_ID, CUSTOMER.FIRST_NAME || ' ' || CUSTOMER.LAST_NAME AS FULL_NAME_C, 
TO_CHAR(RENTAL_DATE,'DD/MM/YYYY') AS DATE_R, TO_CHAR(RETURN_DATE,'DD/MM/YYYY') AS DATE_RE, 
STAFF.FIRST_NAME || ' ' || STAFF.LAST_NAME AS FULL_NAME_E, SUM (RENTAL_RATE) AS TOTAL
FROM CUSTOMER
JOIN RENTAL USING (CUSTOMER_ID)
JOIN RENTAL_DETAIL USING (RENTAL_ID)
JOIN FILM USING (FILM_ID)
JOIN STAFF USING (STAFF_ID)
WHERE RENTAL_ID =  $P{ID_RENTA} 
GROUP BY RENTAL_ID, CUSTOMER.FIRST_NAME, CUSTOMER.LAST_NAME, RENTAL_DATE,RETURN_DATE,STAFF.FIRST_NAME, STAFF.LAST_NAME
ORDER BY RENTAL_DATE]]>
	</queryString>
